#include <iostream>
using namespace std;
int factorial(int num) {
    int fact = 1;
    for (int i = 1; i <= num; i++) {
        fact *= i;
    }
    return fact;
}
bool isStrongNumber(int number) {
    int sum = 0;
    int temp = number;
    while (temp > 0) {
        int digit = temp % 10;  
        sum += factorial(digit); 
        temp /= 10;              
    }
    return (sum == number);
}

int main() {
    int number;
    cout << "Enter a number: ";
    cin >> number;
    if (isStrongNumber(number)) {
        cout << number << " is a Strong Number." << endl;
    } else {
        cout << number << " is not a Strong Number." << endl;
    }

    return 0;
}
