#include <iostream>
using namespace std;

bool isBuzzNumber(int num) {
    return (num % 10 == 7 || num % 7 == 0);
}

int main() {
    int num;
    cout << "Enter a positive integer: ";
    cin >> num;
    if (isBuzzNumber(num)) {
        cout << num << " is a Buzz number." << endl;
    } else {
        cout << num << " is not a Buzz number." << endl;
    }

    return 0;
}
