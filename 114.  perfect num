#include <iostream>
#include <cmath>  
using namespace std;

int main() {
    int num;
    cout << "Enter a number: ";
    cin >> num;
    int sqrt_num = sqrt(num);
    if (sqrt_num * sqrt_num == num) {
        cout << num << " is a perfect square." << endl;
    } else {
        cout << num << " is NOT a perfect square." << endl;
    }

    return 0;
}
