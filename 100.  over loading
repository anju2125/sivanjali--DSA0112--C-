#include <iostream>
using namespace std;
int add(int a, int b) {
    return a + b;
}
double add(double a, double b) {
    return a + b;
}
int add(int a, int b, int c) {
    return a + b + c;
}

int main() {
    cout << "Addition of two integers: " << add(5, 10) << endl;
    cout << "Addition of two doubles: " << add(3.5, 2.8) << endl;
    cout << "Addition of three integers: " << add(1, 2, 3) << endl;

    return 0;
}
